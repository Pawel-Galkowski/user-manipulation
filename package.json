{
  "name": "node.js-mailing",
  "main": "server.js",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "start": "node server.js --prefix server",
    "server": "nodemon server --prefix server",
    "client": "npm start --prefix client",
    "dev": "npm run server && npm run client",
    "installationClient": "npm run reinstallation --prefix client",
    "installationServer": "npm run reinstallation --prefix server",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "description": "Mailing app created by ITGalkowski",
  "author": "Pawel Galkowski",
  "dependencies": {
    "concurrently": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
